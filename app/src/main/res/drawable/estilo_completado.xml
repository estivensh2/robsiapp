<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape  android:shape="rectangle">
            <solid android:color="@color/colorVerdeOscuro" />
            <corners android:radius="5dp" />
        </shape>
    </item>
    <item android:bottom="2dp">
        <shape  android:shape="rectangle">
            <solid android:color="@color/colorVerdeClaro" />
            <stroke android:width="1dp" android:color="@color/colorVerdeOscuro"/>
            <corners android:radius="5dp" />
            <padding android:left="5dp" android:top="15dp" android:right="5dp" android:bottom="15dp"/>
        </shape>
    </item>
</layer-list>
